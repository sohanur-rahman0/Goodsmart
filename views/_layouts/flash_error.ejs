<% if (messages.danger) { %>
    <div class="alert"><span class="closebtn" onclick="this.parentElement.style.display='none';" style="display: none;">&times;</span><%= messages.danger %></div>
<% } %>
<% if (messages.success) { %>
    <div class="alert success"><span class="closebtn" onclick="this.parentElement.style.display='none';" style="display: none;">&times;</span><%= messages.success %></div>
<% } %>